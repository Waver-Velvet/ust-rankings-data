{
  "name": "prof-at-ust",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "update:review": "cd data-review && git fetch && git reset --hard",
    "update:quota": "cd data-schedule-quota && git fetch && git reset --hard",
    "update": "pnpm update:review && pnpm update:quota",
    "run": "tsx index.ts",
    "lint": "eslint *.ts --fix",
    "build": "ncc build index.ts",
    "bundle": "pnpm lint && pnpm build",
    "run:dist": "pnpm bundle && node dist/index.js",
    "prepare": "husky"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "license": "MIT",
  "dependencies": {
    "glob": "^10.3.10",
    "lodash": "^4.17.21",
    "mathjs": "^12.3.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.1",
    "husky": "^9.0.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
